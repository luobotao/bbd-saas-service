<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="bbd-saas-consumer" owner="saas" organization="bbd"/>
   	<!--  启动时不检查提供者 -->
	<dubbo:consumer check="false"></dubbo:consumer>
    <dubbo:registry address="zookeeper://127.0.0.1:2181"/>
    <!--<dubbo:registry address="zookeeper://182.92.227.140:2181"/>-->
    <dubbo:reference id="adminUserService" interface="com.bbd.saas.api.AdminUserService"/>
    <!--订单Service-->
    <dubbo:reference id="orderService" interface="com.bbd.saas.api.OrderService"/>
    <!--用户Service-->
    <dubbo:reference id="userService" interface="com.bbd.saas.api.UserService"/>
    <!--site Service-->
    <dubbo:reference id="siteService" interface="com.bbd.saas.api.SiteService"/>

</beans>